<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd" https="true">
  <meta>
    <author>RapidFTR</author>
    <description>user session resource</description>
  </meta>
  <bindings>
    <insert itemPath="" produces="JSON" >
      <urls>
        <url>http://dev.rapidftr.com:3000/sessions.json</url>
      </urls>
      <inputs>
        <value id="user_name" type="xs:string" paramType="variable" required="true" />
        <value id="password" type="xs:string" paramType="variable" required="true" />
      </inputs>
      <execute>
<![CDATA[

response.object = request.post("user_name=" + inputs["user_name"] + "&password=" + inputs["password"]).response;

]]>
      </execute>
    </insert>
  </bindings>
</table>